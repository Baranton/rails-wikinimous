<h1>Articles</h1>

<%= link_to "Create new article", new_article_path%>

<ul>
<% @articles.each do |article| %>
<li><%= link_to "#{article.title}", article_path(article.id)%></li>
<%= link_to "update", edit_article_path(article.id)%> |
<%= link_to "delete", "/articles/#{article.id}", method: :delete %>
<br><br><br>
<% end %>
</ul>


